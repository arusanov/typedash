{
  "compilerOptions": {
    "outDir": "lib",
    "moduleResolution": "node",
    "module": "es6",
    "target": "es5",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "importHelpers": true,
    "lib": [
      "es5",
      "dom"
    ],
    "strict": true,
    "sourceMap": true,
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "include": [
    "src"
  ]
}
